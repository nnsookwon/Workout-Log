<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nnsookwon.weight_liftinglog"
    android:versionCode="5"
    android:versionName="1.3.1" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nnsookwon.workout_log.SplashActivity"
            android:screenOrientation="nosensor"
            android:theme="@style/Splash_Theme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nnsookwon.workout_log.MyApplication"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="com.nnsookwon.APPLICATION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nnsookwon.workout_log.AddNewExercise"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="com.nnsookwon.ADD_NEW_EXERCISE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nnsookwon.workout_log.EditEntry"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="com.nnsookwon.EDIT_ENTRY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nnsookwon.workout_log.ViewExerciseHistory"
            android:screenOrientation="nosensor" >
            <intent-filter>
                <action android:name="com.nnsookwon.VIEW_EXERCISE_HISTORY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--
     ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>